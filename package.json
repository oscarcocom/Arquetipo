{
  "name": "react-base-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.20",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "axios": "0.22.0",
    "axios-mock-adapter": "^1.20.0",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "redux": "^4.2.0",
    "sass": "^1.54.6",
    "swr": "^1.3.0",
    "web-vitals": "^2.1.3",
    "whatwg-fetch": "^3.6.2"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint-fix": "eslint \"src/**/*.{ts,tsx}\" --quiet --fix",
    "start": "react-scripts start",
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:development": "env-cmd -f .env.development react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "start:production": "env-cmd -f .env.production react-scripts start",
    "build": "react-scripts build",
    "build:local": "env-cmd -f .env.local react-scripts build",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "jest --watchAll",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest-environment-jsdom": "^28.1.3",
    "ts-jest": "^28.0.7",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
